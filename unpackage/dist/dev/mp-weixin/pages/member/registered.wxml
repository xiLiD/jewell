<view class="container"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="t-box"><view class="t-item"><input class="t-input" type="text" name="tel" placeholder="请输入手机号码" data-event-opts="{{[['input',[['__set_model',['','tel','$event',[]]]]]]}}" value="{{tel}}" bindinput="__e"/></view><view class="t-item"><input class="t-input" type="text" name="code" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/><block wx:if="{{showText==true}}"><button data-event-opts="{{[['tap',[['sendCode',['$event']]]]]}}" class="seehouse_get_nub" bindtap="__e">获取验证码</button></block><block wx:else><view class="sends">{{"("+second+"秒)重新获取"}}</view></block></view><view class="t-item"><input class="t-input" type="password" name="passWord" placeholder="用户密码" data-event-opts="{{[['input',[['__set_model',['','passWord','$event',[]]]]]]}}" value="{{passWord}}" bindinput="__e"/></view><view class="t-item"><input class="t-input" type="password" name="passWorda" placeholder="确认密码" data-event-opts="{{[['input',[['__set_model',['','passWorda','$event',[]]]]]]}}" value="{{passWorda}}" bindinput="__e"/></view><view class="t-item"><input class="t-input" name="nvitationCode" placeholder="邀请码" data-event-opts="{{[['input',[['__set_model',['','nvitationCode','$event',[]]]]]]}}" value="{{nvitationCode}}" bindinput="__e"/></view></view><view class="b-box"><button class="blt-botton" formType="submit">提交注册</button></view></form></view>