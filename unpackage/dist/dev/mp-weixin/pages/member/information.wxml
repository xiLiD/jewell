<view class="container"><form data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="t-box"><view class="t-item"><input class="t-input" type="text" name="phone" placeholder="请输入手机号码" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/></view><view class="t-item"><input class="t-input" type="text" name="code" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/><block wx:if="{{showText==true}}"><button data-event-opts="{{[['tap',[['sendCode',['$event']]]]]}}" class="seehouse_get_nub" bindtap="__e">获取验证码</button></block><block wx:else><view class="sends">{{"("+second+"秒)重新获取"}}</view></block></view><view class="t-item"><input class="t-input" name="id_card_name" placeholder="身份证名" data-event-opts="{{[['input',[['__set_model',['','id_card_name','$event',[]]]]]]}}" value="{{id_card_name}}" bindinput="__e"/></view><view class="t-item"><input class="t-input" type="idcard" name="id_card" placeholder="身份证号码" data-event-opts="{{[['input',[['__set_model',['','id_card','$event',[]]]]]]}}" value="{{id_card}}" bindinput="__e"/></view><view class="t-item"><input class="t-input" type="password" name="pay_paw" placeholder="支付密码" data-event-opts="{{[['input',[['__set_model',['','pay_paw','$event',[]]]]]]}}" value="{{pay_paw}}" bindinput="__e"/></view><view class="t-item"><input class="t-input" type="password" name="pay_pawa" placeholder="确认支付密码" data-event-opts="{{[['input',[['__set_model',['','pay_pawa','$event',[]]]]]]}}" value="{{pay_pawa}}" bindinput="__e"/></view></view><view class="t-agree"><switch style="transform:scale(0.7);" type="checkbox" checked="{{agreeCheck}}" color="#201f24" data-event-opts="{{[['change',[['checkChange',['$event']]]]]}}" bindchange="__e"></switch>已阅读并同意<text data-event-opts="{{[['tap',[['dump',['/pages/member/protocol']]]]]}}" class="red" bindtap="__e">《注册协议说明》</text></view><view class="b-box"><button class="blt-botton" formType="submit">确认提交</button></view></form></view>