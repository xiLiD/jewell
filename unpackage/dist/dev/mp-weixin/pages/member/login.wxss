
page {
	background-color: #ffffff !important;
}
.t-box {
	background-color: #ffffff;
	width: 92%;
	margin-left: 4%;
	padding: 11.25rpx 0 7.5rpx 0;
	margin-top: 37.5rpx;
}
.t-item {
	border-bottom: 1px solid #efefef;
	margin: 37.5rpx 0 56rpx 0;
	clear: both;
}
.t-t {
	width: 150rpx;
	text-align: right;
	float: left;
	font-size: 30rpx;
}
.t-input {
	margin: 19rpx 0 3.75rpx 0;
	height: 49rpx;
	line-height: 49rpx;
	font-size: 26rpx;
}
.b-box {
	width: 92%;
	margin-left: 4%;
}
.blt-botton {
	color: #f8d79f;
	background-color: #201f24;
	margin-bottom: 19rpx;
	width: 100%;
	text-align: center;
	height: 86rpx;
	line-height: 86rpx;
	border-radius: 11rpx;
	font-size: 30rpx;
}
.blt-r {
	color: #5e5e5e;
	font-size: 22.5rpx;
	float: left;
}
.blt-z {
	color: #5e5e5e;
	font-size: 22.5rpx;
	float: right;
}
.bt-x {
	position: fixed;
	bottom: 75rpx;
	width: 100%;
	text-align: center;
	font-size: 22.5rpx;
}
.bt-x text {
	color: #007aff;
}
.b-phone {
	position: fixed;
	bottom: 60rpx;
}

