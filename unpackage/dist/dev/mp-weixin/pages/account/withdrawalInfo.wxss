
.container,
page {
	background-color: #f4f5f6;
}
.t_se {
	width: 750rpx;
	margin: 0 auto;
	margin-top: 10rpx;
	background-color: #FFFFFF;
	padding: 0 30rpx;
}
.t-main {
	width: 750rpx;
	padding-top: 20rpx;
}
.main-title {
	width: 750rpx;
	background-color: #FFFFFF;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 100rpx;
	margin-bottom: 10rpx;
}
.main-title view {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	text-align: center;
	height: 100rpx;
	line-height: 100rpx;
	color: #999999;
	font-size: 28rpx;
}
.uni-loadmore {
	clear: both;
}
.main-item {
	width: 750rpx;
	background-color: #FFFFFF;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 160rpx;
	border-bottom: 1px solid #EFEFEF;
	padding-top: 10rpx;
}
.main-item .item-left {
	float: left;
	width: 120rpx;
}
.item-left image {
	width: 80rpx;
	height: 80rpx;
	margin: 20rpx;
}
.item-center{
	width: 450rpx;
}
.item-center .item-title{
	font-weight: 600;
	font-size: 28rpx;
}
.item-center .item-date{
	color: #666666;
	font-size: 25rpx;
}
.item-center .item-intro{
	color: #666666;
	font-size: 25rpx;
}
.item-right{
	width: 180rpx;
	text-align: center;
}
.item-right .item-price{
	font-weight: 600;
	font-size: 36rpx;
	text-align: center;
}
.item-right .item-status{
	font-size: 25rpx;
	text-align: center;
}

