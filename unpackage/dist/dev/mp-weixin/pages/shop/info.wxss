	font-size: 25rpx;
	color: #333;
	border-radius: 8rpx;
	float: left;
	margin-bottom: 15rpx;
	margin-right: 20rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	white-space: nowrap;
}
.sku-center .sku-item .active {
	border: 1px solid #dc0000;
	color: #dc0000;
	background: #fff;
}
.sku-bt {
	width: 100%;
